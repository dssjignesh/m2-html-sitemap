<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="header.links">
            <block class="Dss\HtmlSiteMap\Block\ItemsCollection" name="custom_block_top"
                as="custom_block_top" template="Dss_HtmlSiteMap::custom_top.phtml" before="-"/>
        </referenceBlock>
        <referenceBlock name="footer_links">
            <block class="Dss\HtmlSiteMap\Block\ItemsCollection" name="custom_block"
                as="custom_block" template="Dss_HtmlSiteMap::custom.phtml" after="-"/>
        </referenceBlock>
    </body>
</page>
